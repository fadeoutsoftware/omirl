<!-- Custom JavaScript for the Side Menu and Smooth Scrolling -->
<script>
    $("#menu-close").click(function(e) {
        e.preventDefault();
        $("#sidebar-wrapper").toggleClass("active");
    });
</script>
<script>
    $("#menu-toggle").click(function(e) {
        e.preventDefault();
        $("#sidebar-wrapper").toggleClass("active");
    });
</script>
<script>
    $(function() {
        $('a[href*=#]:not([href=#])').click(function() {
            if (location.pathname.replace(/^\//,'') == this.pathname.replace(/^\//,'')
                    || location.hostname == this.hostname) {

                var target = $(this.hash);
                target = target.length ? target : $('[name=' + this.hash.slice(1) +']');
                if (target.length) {
                    $('html,body').animate({
                        scrollTop: target.offset().top
                    }, 1000);
                    return false;
                }
            }
        });
    });

    function OmirlMoveTo(sHash)
    {
        var header = $("#top");
        var mainContainer = $("#contentcontainer");
        var body = $("body");
        
        var iScrollAmout = header.height();
        
        
        // On 'non small' devices we cannot use a const scroll
        // amount, it should be calculated dynamically
        var sClass = "header-collapsed";
        var sClassOnMobile = "header-collapsed-mobile";
        
        
        if( body.css("overflow-y") == "hidden")
        {
            // ON LARGE SCREEN DEVICES
            header.removeClass(sClassOnMobile);
            
            
            sHash = sHash.replace("#", "");
            if(sHash == "top")
            {
                //header.removeClass(sClass);
                //mainContainer.removeClass(sClass);
                iScrollAmout = 0;
                header.css({transform : "translateY(-"+ iScrollAmout +"px)"});
                mainContainer.css({transform : "translateY(-"+ iScrollAmout +"px)"});
            }
            else
            {
                //header.addClass(sClass);
                //mainContainer.addClass(sClass);
                
                header.css({transform : "translateY(-"+ iScrollAmout +"px)"});
                mainContainer.css({transform : "translateY(-"+ iScrollAmout +"px)"});
            }
        }
        else
        {
            
            sHash = sHash.replace("#", "");
            if(sHash == "top")
            {
                // Show header, hide content
                header.removeClass(sClassOnMobile);
                mainContainer.addClass(sClassOnMobile);
            }
            else
            {
                // Hide header, show content
                header.addClass(sClassOnMobile);
                mainContainer.removeClass(sClassOnMobile);
            }
            return;
            
            
            var target = $(sHash);
            target = target.length ? target : $('[name=' + this.hash.slice(1) +']');
            if (target.length) {
                $('html,body').animate({
                    scrollTop: target.offset().top
                }, 1000);
                return false;
            }
        }

    }
</script>
<script>   
    $(resizeMap());
    $(window).resize(resizeMap);

    // Avoid scrolling on small devices
    window.setTimeout(function(){
        var body = $("body");
        if( body.css("overflow-y") != "hidden")
        {
            // ON MED/SMALL SCREEN DEVICES
            OmirlMoveTo("top");
        }
    }, 1000);
    

    function resizeMap(){
        var mapHeight = $("#top").height();// - $("#yr-map-header").outerHeight() - $("#yr-map-footer").outerHeight();
        var mapWidth = $("#top").width();

        if ($("#omirlMap").length>0) {
            $("#contentcontainer").height(mapHeight);
            $("#omirlMap").height(mapHeight);
            $("#omirlMap").width(mapWidth);
            console.log( 'RESIZE MAP SET HEIGHT' );
        }
        else {
            $("#contentcontainer").height("100%");
            console.log( 'RESIZE MAP SET 100%' );
        }
    }
</script>

